#: prefs.js:34
msgid "Main Settings"
msgstr "メイン設定"

#: prefs.js:43
msgid "Dock is fixed and always visible"
msgstr "Dock を固定して常に表示する"

#: prefs.js:59
msgid "Animation time [ms]"
msgstr "アニメーション時間 (ミリ秒)"

#: prefs.js:70
msgid "Show delay [ms]"
msgstr "表示するまでの間隔 (ミリ秒)"

#: prefs.js:81
msgid "Hide delay [ms]"
msgstr "非表示までの間隔 (ミリ秒)"

#: prefs.js:96
msgid "Autohide"
msgstr "自動非表示"

#: prefs.js:103
msgid "intellihide"
msgstr "インテリジェント非表示"

#: prefs.js:131
msgid "Application based intellihide"
msgstr ""
"アクティブなアプリケーションに対してだけインテリジェント非表示を有効にする"

#: prefs.js:149
msgid "Require pressure to show the dock"
msgstr ""

#: prefs.js:159
msgid "Pressure threshold (px)"
msgstr ""

#: prefs.js:198
msgid "Show the dock on following monitor (if attached)"
msgstr "Dock を表示するモニター (もしあれば)"

#: prefs.js:200
msgid "Primary (default)"
msgstr "プライマリ (既定)"

#: prefs.js:201
msgid "1"
msgstr "1"

#: prefs.js:202
msgid "2"
msgstr "2"

#: prefs.js:203
msgid "3"
msgstr "3"

#: prefs.js:204
msgid "4"
msgstr "4"

#: prefs.js:229
msgid "Max height"
msgstr "最大の高さ"

#: prefs.js:247
msgid "Expand (experimental and buggy)"
msgstr "拡張する (実験的で不具合あり)"

#: prefs.js:279
msgid "Maximum icon size"
msgstr "アイコンの最大サイズ"

#: prefs.js:284
msgid "16"
msgstr "16"

#: prefs.js:285
msgid "24"
msgstr "24"

#: prefs.js:286
msgid "32"
msgstr "32"

#: prefs.js:287
msgid "48"
msgstr "48"

#: prefs.js:288
msgid "64"
msgstr "64"

#: prefs.js:310
msgid "Show favorite application icons"
msgstr "お気に入りのアプリケーションアイコンを表示する"

#: prefs.js:315
msgid "Show running application icons"
msgstr "実行中のアプリケーションアイコンを表示する"

#: prefs.js:320
msgid "Show applications button at the top"
msgstr ""

#: prefs.js:337
msgid "Optional features"
msgstr "オプション機能"

#: prefs.js:342
msgid "Apply custom theme (work only with the default Adwaita theme)"
msgstr ""

#: prefs.js:358
msgid "Customize the dock background opacity"
msgstr "Dock の背景の不透明度をカスタマイズする"

#: prefs.js:375
msgid "Opacity"
msgstr "不透明度"

#: prefs.js:403
msgid "Switch workspace when scrolling over the dock"
msgstr "Dock 上でスクロールするとワークスペースを切り替える"

#: prefs.js:414
msgid "Switch one workspace at a time"
msgstr "一度に切り替えるワークスペースをひとつだけにする"

#: prefs.js:423
msgid "Deadtime between each workspace switching [ms]"
msgstr ""

#: prefs.js:434
msgid "Only a 1px wide area close to the screen edge is active"
msgstr "画面端に接する 1px の領域だけスクロールに反応させる"

#: prefs.js:440
msgid "All the area of the dock is active"
msgstr "Dock 上のすべての領域をスクロールに反応させる"

#: prefs.js:469
msgid "Customize actions on mouse click"
msgstr "クリック時の挙動をカスタマイズする"

#: prefs.js:484
msgid "Action on clicking on running app"
msgstr "起動中アプリケーションのクリック時の挙動"

#: prefs.js:486
msgid "Do nothing (default)"
msgstr "なにもしない (既定)"

#: prefs.js:487
msgid "Minimize"
msgstr "最小化する"

#: prefs.js:488
msgid "Launch new window"
msgstr "新しいウィンドウで開く"

#: prefs.js:489
msgid "Cycle through application windows"
msgstr "アプリケーションのウィンドウを切り替える"

#: prefs.js:500
msgid "Minimize window on shift+click (double click for all app windows)"
msgstr ""
"Shift+クリックでウィンドウを最小化 (ダブルクリックはアプリケーションの全ウィ"
"ンドウが対象)"

#~ msgid "Only when in autohide"
#~ msgstr "自動非表示のときだけ"
